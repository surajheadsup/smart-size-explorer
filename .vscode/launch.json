{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension (Codium)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "codium",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}
